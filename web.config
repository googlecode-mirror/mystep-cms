<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument enabled="true">
            <files>
                <remove value="default.aspx" />
                <remove value="iisstart.htm" />
                <remove value="Default.asp" />
                <remove value="Default.htm" />
                <add value="index.php" />
            </files>
        </defaultDocument>
        <handlers>
            <add name="PHP" path="*.php" verb="*" modules="IsapiModule" scriptProcessor="E:\server\php\php5isapi.dll" resourceType="Unspecified" />
        </handlers>
        <directoryBrowse enabled="true" />
    </system.webServer>
</configuration>
